@model List<Empleado>

<h1>Session Empleados</h1>

<a asp-controller="Empleados"
    asp-action="EmpleadosAlmacenados">
    Empleados Almacenados
</a>

<br/>
<h1 style="color:red">
    @ViewData["MENSAJE"]
</h1>

<ul class="list-group">
    @foreach (Empleado emp in Model)
    {
        <li class="list-group-item list-group-item-info">
            @emp.Apellido
            <a asp-controller="Empleado"
                asp-action="SessionEmpleado"
                asp-route-action="@emp.IdEmpleado">
                Almacenar Empleado
            </a>
        </li>
    }
</ul>