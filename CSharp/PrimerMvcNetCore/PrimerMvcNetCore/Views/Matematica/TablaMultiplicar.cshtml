@model List<int>

<h1>Tabla multiplicar</h1>

<form method="post">
    <label>Introduce numero: </label>
    <input type="number" name="numero"
        class="form-control" required/>
    <button class="btn btn-success">
        Calcular
    </button>
</form>

@if(Model != null)
{
    <ul>
        @foreach(int num in Model)
        {
            <li>@num</li>
        }
    </ul>
}
