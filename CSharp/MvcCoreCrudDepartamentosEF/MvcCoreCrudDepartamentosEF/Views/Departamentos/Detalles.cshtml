@model Departamento

<h1>Departamento</h1>

<table class="table table bordered">
    <thead>
        <tr>
            <td>Numero departamento</td>
            <td>Nombre</td>
            <td>Localidad</td>
        </tr>
    </thead>
    <tbody>     
        <tr>
            <td>@Model.IdDeptartamento</td>
            <td>@Model.Nombre</td>
            <td>@Model.Localidad</td>
        </tr>
    </tbody>
</table>