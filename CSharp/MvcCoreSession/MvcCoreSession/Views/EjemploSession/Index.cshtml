@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1>Proyecto Session</h1>
    <p>Hora: @Context.Session.GetString("hora")</p>
    <p>Nombre: @Context.Session.GetString("nombre")</p>

    <p>Persona: @Context.Session.Get("PERSONASE")</p>
</div>

<a asp-controller="EjemploSession" asp-action="SessionPersona" class="btn btn-success">
    Persona
</a>

<a asp-controller="EjemploSession" asp-action="ColeccionSessionPersona" class="btn btn-success">
    Coleccion
</a>